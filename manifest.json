{
    "manifest_version": 2,
    "name": "ext-browser-beta",
    "version": "1.0",
    "description": "Description of your extension",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
        }
    },
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}